{
    "name": "mean-mat-server",
    "version": "1.0.0",
    "main": "dist/src/index.js",
    "scripts": {
        "start": "tsx src/index.ts",
        "dev": "nodemon",
        "dev:websocket": "USE_WEBSOCKET=true nodemon",
        "debug": "tsx --inspect src/index.ts",
        "build": "tsc",
        "test": "node --test --import tsx \"test/**/*.test.ts\"",
        "ts-check": "tsc --noEmit",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.{js,ts,html,ejs,scss,css}\"",
        "format:check": "prettier --check \"src/**/*.{js,ts,html,ejs,scss,css}\"",
        "clean": "rm -rf dist node_modules package-lock.json"
    },
    "keywords": [],
    "author": "TechiTech",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "bcrypt": "^6.0.0",
        "cloudinary": "^2.7.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-async-handler": "^1.2.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.17.0",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "socket.io": "^4.8.1",
        "stripe": "^18.4.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "@tsconfig/node22": "^22.0.2",
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.7.0",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.46.0",
        "@typescript-eslint/parser": "^8.46.0",
        "cross-env": "^7.0.3",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.3",
        "globals": "^16.3.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "prettier-plugin-ejs": "^1.0.3",
        "supertest": "^7.1.4",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3"
